        "responses": {
          "201": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/UpvoteResponse"
            }
          },
          "401": {
            "description": "User not authorised",
            "schema": {
              "$ref": "#/definitions/UserNotAuth"
            }
          },
          "404": {
            "description": "Not found",
            "schema": {
