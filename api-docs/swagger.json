      "post": {
        "tags": [
          "/users"
        ],
        "summary": "Create new user on the app",
        "description": "By filling in valid form inputs, a new user can create an account",
        "operationId": "signUp",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "Form input model",
            "required": true,
            "schema": {
              "$ref": "#/definitions/User"
            }
          }
        ],
        "responses": {
          "201": {
            "schema": {
              "$ref": "#/definitions/Signup"
            }
          },
          "422": {
            "description": "First Name is required",
            "schema": {
              "$ref": "#/definitions/SignupValidation"
            }
          },
          "409": {
            "description": "Email already exist",
            "schema": {
              "$ref": "#/definitions/SignupError"
            }
          },
          "default": {
            "description": "An Error Occured/ Error Creating user"
          }
        }
      }
    },
    "/users/signin": {
      "post": {
        "tags": [
          "/users"
        ],
        "summary": "Authenticates and logs user into the app",
        "description": "By providing valid login credentials, a user is able to login to the system",
        "operationId": "signIn",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "Login credentials",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Signin"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "You are now signed in",
            "schema": {
              "$ref": "#/definitions/SignInSuccess"
            }
          },
          "401": {
            "description": "Sign in failed, Wrong email/password",
            "schema": {
              "$ref": "#/definitions/SigninError"
            }
          },
          "422": {
            "description": "Missing data input",
            "schema": {
              "$ref": "#/definitions/SigninError"
            }
          },
          "default": {
            "description": "Error Signing In User"
          }
        }
      }
    },
